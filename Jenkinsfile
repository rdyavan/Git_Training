pipeline{
  agent any
  stages{
    stage('init'){
      steps{
        echo 'Hi... I am trying my own code'
        echo 'let see what will happen'
      }
    }
    stage('Build to stage'){
      steps{
      echo 'It is almost success'
      }
    }
    
    stage('Build to prod'){
      steps{
      echo 'Ohooo, It is done'
      }
    }
  }
  
}
